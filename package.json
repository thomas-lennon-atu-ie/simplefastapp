{
  "name": "simplefastapp",
  "version": "0.0.8",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "test:debug": "jest --detectOpenHandles --runInBand",
    "test:report": "jest --reporters=default --reporters=./jest-custom-reporter.js",
    "build:web": "expo export --platform web",
    "build:android:dev": "eas build --platform android --profile development --non-interactive --no-wait",
    "build:android:staging": "eas build --platform android --profile preview --non-interactive --no-wait",
    "build:android:prod": "eas build --platform android --profile production --non-interactive",
    "build:android:local": "expo prebuild --clean --platform android && cd android && gradlew.bat assembleDebug",
    "postinstall": "echo 'Postinstall completed' || true"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@lottiefiles/dotlottie-react": "^0.6.5",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.3.0",
    "@react-native-firebase/app": "^21.13.0",
    "@react-native-firebase/auth": "^21.14.0",
    "@react-native-google-signin/google-signin": "^13.3.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.5",
    "@react-navigation/native-stack": "^7.3.10",
    "@react-navigation/stack": "^7.2.9",
    "apexcharts": "^4.7.0",
    "expo": "^53.0.9",
    "expo-auth-session": "~6.1.5",
    "expo-dev-client": "~5.1.8",
    "expo-linear-gradient": "~14.1.4",
    "expo-notifications": "^0.29.14",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.12",
    "expo-web-browser": "~14.1.6",
    "firebase": "^9.23.0",
    "lottie-react-native": "7.2.2",
    "moment": "^2.30.1",
    "number-flow": "^0.5.7",
    "react": "19.0.0",
    "react-apexcharts": "^1.7.0",
    "react-datetime": "^3.3.1",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "react-svg": "^16.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/cli": "^18.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.4",
    "@types/react-native": "^0.72.8",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "@eslint/js": "^9.0.0",
    "eslint": "^9.26.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.4.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript-eslint": "^7.3.1",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^53.0.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-svg-transformer": "^1.5.1",
    "react-test-renderer": "^19.0.0",
    "ts-jest": "^29.3.2",
    "typescript": "^5.3.3"
  },
  "private": true,
  "overrides": {
    "@lottiefiles/dotlottie-react": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
